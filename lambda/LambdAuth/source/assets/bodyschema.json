{
"anyOf":[
    {"type":"object",
    "properties":{
        "key":{
            "type":"string",
            "minLength":1
        },
        "algorithm":{
            "type":"string",
            "enum":["aes-256-cbc-hmac-sha256"]
        },
        "payload":{
            "type":"string",
            "minLength":1
        }
    },
    "required":[
        "key","algorithm","payload"
    ],    
    "additionalProperties":false
    },
    {"type":"object",
    "properties":{
        "id":{
            "type":"string",
            "pattern":"^[A-Za-z][A-Za-z0-9._]{3,14}$"
        },
        "token":{
            "type":"sting",
            "pattern":"^[0-9]{6,6}$"
        },
        "B":{
            "type":"sting",
            "minLength":"512",
            "pattern":"^[0-9a-fA-F]+$"
        },
        "hotp":{
            "type":"sting",
            "minLength":"1",
            "pattern":"^[0-9a-fA-F]+$"
        }
    },
    "required":[
        "id","token","B","hotp"
    ],
    "additionalProperties":false
    }
]
}    
