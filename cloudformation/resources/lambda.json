"CreateHealthCheck":{
      "Type" : "AWS::Lambda::Function",
      "Properties" : {
        "Code" : create.zip,
        "FunctionName":{"Fn::Join":["-",[
                "CreateHealthCheck", 
                {"Ref":"AWS::StackName"}
            ]]},
        "Handler" : "handler.handler",
        "MemorySize" : "128",
        "Role" : {"Fn::GetAtt":["CreateHealthCheckRole","Arn"]},
        "Runtime" : "nodejs4.3",
        "Timeout" : "100",
      }
}

