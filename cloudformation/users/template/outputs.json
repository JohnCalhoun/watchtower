     "AdminPoolId":{
        "Description":"The Administrator Cognito Pool ID",
        "Value":{"Fn::GetAtt":["adminUserPool","Id"]},
        "Export":{
            "Name":{"Fn::Join":["-",[
                {"Ref":"AWS::StackName"},
                "AdminPoolId"
            ]]}
        }
    },  
    "RequesterPoolId":{
        "Description":"The Requester Cognito Pool ID",
