            "Name":{"Fn::Join":["-",[
                {"Ref":"AWS::StackName"},
                "RequesterPoolId"
            ]]}
        }
    },
    "ObserverPoolId":{
        "Description":"The Observer Cognito Pool ID",
        "Value":{"Fn::GetAtt":["observerUserPool","Id"]},
        "Export":{
            "Name":{"Fn::Join":["-",[
                {"Ref":"AWS::StackName"},
                "ObserverPoolId"
            ]]}
        }
    }
},
