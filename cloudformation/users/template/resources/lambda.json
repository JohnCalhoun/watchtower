"CreateCognitoPool":{
      "Type" : "AWS::Lambda::Function",
      "Properties" : {
        "Code" : {
            "S3Bucket":{"Ref":"ArtifactsBucket"},
            "S3Key":"disclosure/lambda/lambda-createcognitopool.zip"
        },
        "FunctionName":{"Fn::Join":["-",[
                "CreateCognitoPool", 
                {"Ref":"AWS::StackName"}
            ]]},
        "Handler" : "handler.handler",
        "MemorySize" : "128",
        "Role" : {"Fn::GetAtt":["CreateCognitoPoolRole","Arn"]},
        "Runtime" : "nodejs4.3",
        "Timeout" : "100"
      }
},
